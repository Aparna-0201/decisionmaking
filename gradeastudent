include<stdio.h>
include<conio.h>
void main()
{
int mark;
printf("enter student mark\n");
scanf("%d",&mark);
if(mark>=85)
{
printf("GRADE A");
}
else if(mark>=70 && mark<85)
{
printf("GRADE B");
}
else if(mark>=55 && mark<70)
{
printf("GRADE C");
}
else if(mark>=40 && mark<55)
{
printf(" GRADE D");
}
else{
printf("fail");
}
}
